	char* category;
	char* type;
	int mem_size;
	float latency;
	float frequency;
	int width;
	float area;
	float static_power;
	float active_energy;
	float read_energy;
	float write_energy;
	float read_size_energy;
	float write_size_energy;
	char* component_specific;

<?xml version="1.0" encoding="UTF-8"?>
<design_space xmlns="http://www.multicube.eu/" version="1.3">
	<simulator>
        <simulator_executable path="/usr/bin/python @image@/examples/beverage_can/beverage_can.py" />
	</simulator>
	<parameters>
		<parameter name="radius" description="cm" type="integer" min="2" max="10" step="2" />
		<parameter name="height" description="cm" type="integer" min="10" max="30" step="5"/>
		<parameter name="thickness" description="mm" type="integer" min="1" max="5" step="1"/>
	</parameters>
	<system_metrics>
		<system_metric name="surface" type="integer" unit="cm2" desired="small" />
		<system_metric name="volume" type="integer" unit="cm3" desired="big" />
    </system_metrics>
</design_space>
